{
  "expo": {
    "name": "NovlNest",
    "slug": "NovlNest",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "novlnest",
    "deepLinks": true,
    "deepLinkingStrategy": "managed",
    "splash": {
      "image": "./assets/images/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F7FF",
      "dark": {
        "image": "./assets/images/app-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0D0D1A"
      }
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.tolu.novlnest",
      "splash": {
        "image": "./assets/images/app-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F8F7FF",
        "dark": {
          "image": "./assets/images/app-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#0D0D1A"
        }
      },
      "associatedDomains": [
        "applinks:novlnest.com",
        "applinks:auth.expo.io",
        "applinks:novlnest.com",
        "applinks:auth.expo.io"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.171117861268-nj2vicso0ia0ss2jj3vllago0dbm9lno"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.novlnest",
      "splash": {
        "image": "./assets/images/app-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F8F7FF",
        "dark": {
          "image": "./assets/images/app-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#0D0D1A"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "novlnest",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "novlnest.com",
              "pathPrefix": "/auth/action"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f2e819b4-a761-4e20-be94-b97702c6de46"
      }
    }
  }
}